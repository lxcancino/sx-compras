import sx.core.Producto

model {
	Producto producto
}

json g.render(producto, [resolveTemplate: false]) {
	linea {
		id producto?.linea?.id
		linea producto.linea?.linea
	}
	if(producto.proveedorFavorito) {
		proveedorFavorito {
			id: producto.proveedorFavorito.id
			nombre producto.proveedorFavorito.nombre
			tipo producto.proveedorFavorito.tipo
		}
	}
}
